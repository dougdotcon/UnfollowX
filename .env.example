# Twitter/X API Credentials
# Obtenha suas credenciais em: https://developer.twitter.com/
TWITTER_API_KEY=your_api_key_here
TWITTER_API_SECRET=your_api_secret_here
TWITTER_ACCESS_TOKEN=your_access_token_here
TWITTER_ACCESS_TOKEN_SECRET=your_access_token_secret_here

# OpenRouter API Key
# Obtenha sua chave em: https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Configurações Opcionais
# Número de unfollows por execução (padrão: 20)
UNFOLLOWS_PER_BATCH=20

# Intervalo entre execuções em minutos (padrão: 20)
EXECUTION_INTERVAL_MINUTES=20

# Intervalo entre unfollows em segundos (padrão: 3)
UNFOLLOW_DELAY=3

# Modo de filtro: normal ou aggressive (padrão: normal)
FILTER_MODE=normal
